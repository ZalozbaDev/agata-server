{
  "name": "agata-server",
  "version": "1.0.0",
  "description": "Express server with TypeScript and Mongoose",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "pm2:deploy:app": "npm run build && pm2 start app.json",
    "pm2:start:app": "pm2 start app.json",
    "pm2:stop:app": "pm2 stop app.json",
    "pm2:destroy:app": "pm2 delete app.json"
  },
  "keywords": [
    "express",
    "typescript",
    "mongoose",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openai/agents": "^0.1.9",
    "@tensorflow/tfjs-node": "^4.22.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "node-vad": "^1.1.4",
    "openai": "^5.9.0",
    "pdf-parse": "^1.1.1",
    "wav-decoder": "^1.3.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.5",
    "@types/node": "^20.10.5",
    "@types/pdf-parse": "^1.1.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
